{
  "name": "@redux-cbd/core",
  "version": "1.0.4",
  "description": "Declarative class-based annotations for redux. OOP style for your typescript-react-redux app.",
  "main": "./index.js",
  "private": false,
  "scripts": {
    "test": "npx tsc -p ./test/tsconfig.json && npx jest --all --collectCoverageFrom=src/**/*.ts --config ./test/jest.config.js --detectOpenHandles && npx rimraf ./test/jest.config.js",
    "build": "tsc -p ./src/tsconfig.json",
    "prepare": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neloreck/redux-cbd.git"
  },
  "keywords": [
    "action",
    "class-based",
    "connect",
    "declarative",
    "decorator",
    "metadata",
    "oop",
    "react",
    "reducer",
    "redux",
    "reflection",
    "reflect-metadata",
    "state",
    "typescript",
    "utility"
  ],
  "author": "Syrotenko Igor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neloreck/redux-cbd/issues"
  },
  "homepage": "https://github.com/neloreck/redux-cbd#readme",
  "peerDependencies": {
    "reflect-metadata": "^0.1.9"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.9",
    "redux": "^4.0.1",
    "react": "^16.6.3",
    "react-redux": "^5.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.7",
    "@types/react-redux": "^6.0.10",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.6"
  },
  "directories": {
    "test": "test"
  }
}
