{
  "name": "redux-cbd-example",
  "author": "Syrotenko Igor",
  "private": false,
  "version": "1.3.4",
  "main": "src/index.tsx",
  "scripts": {
    "start": "npx webpack-dev-server --config ./webpack.config.ts",
    "build": "webpack --config ./webpack.config.ts"
  },
  "babel": {
    "minified": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8",
            "browsers": [
              "last 50 versions"
            ]
